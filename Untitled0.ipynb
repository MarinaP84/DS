      "source": [
        "text = input(\"Введите строку: \")\n",
        "vowels = \"аеёиоуыэюя\"\n",
        "vowel_count = 0\n",
        "\n",
        "for char in text.lower():\n",
        "    if char in vowels:\n",
        "        vowel_count += 1\n",
        "\n",
        "print(f\"Количество гласных букв: {vowel_count}\")"
      ],
